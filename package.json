{
	"name": "@umd-mith/iiif-media-parsers",
	"version": "0.3.0",
	"description": "TypeScript utilities for parsing IIIF media fragments, ranges, and WebVTT speaker annotations",
	"keywords": [
		"iiif",
		"media-fragments",
		"w3c",
		"typescript",
		"annotation",
		"webvtt",
		"range",
		"presentation-api"
	],
	"homepage": "https://github.com/umd-mith/iiif-media-parsers#readme",
	"bugs": {
		"url": "https://github.com/umd-mith/iiif-media-parsers/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/umd-mith/iiif-media-parsers.git"
	},
	"license": "BSD-3-Clause-Clear",
	"author": "Trevor Munoz <tmunoz@umd.edu>",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsc",
		"test": "vitest",
		"test:ci": "vitest run",
		"type-check": "tsc --noEmit",
		"lint": "eslint src/",
		"lint:fix": "eslint src/ --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"build:docs": "node scripts/build-docs.js",
		"release": "./scripts/release.sh",
		"prepublishOnly": "pnpm run build && pnpm run test:ci",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{ts,js}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,md,yml,yaml}": [
			"prettier --write"
		]
	},
	"sideEffects": false,
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"prettier": "^3.7.4",
		"typescript": "^5.7.0",
		"typescript-eslint": "^8.52.0",
		"vitest": "^3.0.0"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
